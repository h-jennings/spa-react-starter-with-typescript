{
  "name": "spa-react-starter-with-typescript",
  "version": "1.0.0",
  "description": "Spa react starter with typescript",
  "main": "index.tsx",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config config/webpack.dev.js",
    "start": "npm run dev",
    "build": "cross-env NODE_ENV=production webpack --config config/webpack.prod.js",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint-and-fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.{ts, .tsx, .js, .jsx}' --write"
  },
  "author": "Hunter Jennings",
  "keywords": [
    "react",
    "react-hooks",
    "typescript"
  ],
  "license": "ISC",
  "devDependencies": {
    "@types/node-sass": "^4.11.0",
    "@types/prettier": "^2.0.0",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "@types/react-router": "^5.1.7",
    "@types/react-router-dom": "^5.1.5",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^6.0.3",
    "css-loader": "^3.4.0",
    "css-modules-typescript-loader": "^4.0.0",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^6.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-custom-properties": "^9.0.2",
    "postcss-loader": "^3.0.0",
